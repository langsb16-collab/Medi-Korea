{
  "$schema": "node_modules/wrangler/config-schema.json",
  "name": "medi-trip-korea",
  "compatibility_date": "2025-11-30",
  "pages_build_output_dir": "./dist",
  "compatibility_flags": [
    "nodejs_compat"
  ],
  "d1_databases": [
    {
      "binding": "DB",
      "database_name": "webapp-production",
      "database_id": "local-d1-database"
    }
  ],
  "r2_buckets": [
    {
      "binding": "UPLOADS",
      "bucket_name": "webapp-uploads"
    }
  ]
}